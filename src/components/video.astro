---
import AstroImage from '../svg/astro/full-logo-light.svg'
import TailwindImage from '../svg/undraw/undraw_tailwind_css.svg'
import CoreImage from '../svg/undraw/undraw_static_assets.svg'
import HeroImg from '../components/findoutmoreimg.astro'
import MulchImg from '../images/Mulch2.png'

const callouts = [
  {
    name: 'Mowing',
    description: 'Our mowing service includes mowing, trimming, and blowing of all concrete surfaces.',
    imageSrc: HeroImg,
    imageAlt: 'Astro logo',
    href: 'https://astro.build',
  },
  {
    name: 'Leaf Removal',
    description:
      'We offer one time or recurring cleanups. We also offer curbside pickup.',
    imageSrc: TailwindImage,
    imageAlt: 'TailwindCSS drawing',
    href: 'https://tailwindcss.com',
  },
  {
    name: 'Mulch Installation',
    description: 'Our mulch service includes weeding.',
    imageSrc: CoreImage,
    imageAlt: 'Core Web',
    href: 'https://w3.org',
  },
]
---

<div class="bg-grey-100 dark:bg-grey-900">
  <div class="max-w-7xl mx-auto px-4 sm:px-6 lg:px-8">
    <div class="max-w-2xl mx-auto py-16 sm:py-24 lg:py-32 lg:max-w-none">
      <h2 class="text-4xl font-extrabold text-sky-800 dark:text-sky-100">
        
     
          <center><iframe width="100%" height="600" src="https://www.youtube.com/embed/Fq0CxPzye6s?autoplay=1&mute=1" frameborder="0" allowfullscreen></iframe></center>
          
          
          
      </div>
      
    </div>
  </div>
</div>
